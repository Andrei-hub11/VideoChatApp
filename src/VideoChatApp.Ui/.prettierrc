{
  "importOrder": [
    "^@core/(.*)$",
    "<THIRD_PARTY_MODULES>",
    ".scss",
    ".css",
    "store/",
    "utils/",
    "components/",
    "^./(.*)$",
    "(components/|./index)",
    "\\.(svg|png|jpe?g|gif|webp)$",
    "^../(.*)$",
    "(?=./styles.module.scss)"
  ],
  "importOrderSeparation": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
